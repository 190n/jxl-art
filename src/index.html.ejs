    <pre id="log"></pre>
  </div>
  <div style="overflow: auto">
    <canvas id="cvs" width="1024" height="1024"></canvas>
  </div>
</main>
<footer>
  Made with 🤪 by <a href="https://twitter.com/dassurma">Surma</a>.
</footer>
<script src="<%= emitChunk('src/main.js') %>"></script>
